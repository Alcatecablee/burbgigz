modules = ["nodejs-20", "web"]
[agent]
expertMode = true
integrations = ["javascript_supabase:1.0.0", "javascript_log_in_with_replit:1.0.0"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "vm"
build = ["npm", "run", "build"]
run = ["NODE_ENV=production", "tsx", "server/index.ts"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=development npx tsx server/index.ts"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 4200

[[ports]]
localPort = 35303
externalPort = 5173

[[ports]]
localPort = 38269
externalPort = 3000

[[ports]]
localPort = 41679
externalPort = 3001

[[ports]]
localPort = 43553
externalPort = 3002

[[ports]]
localPort = 45081
externalPort = 3003
